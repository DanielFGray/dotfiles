#!/usr/bin/env bash

if [[ -e ${HOME}/.bash_utils ]]; then
	source ${HOME}/.bash_utils
else
	echo -e "\e[31mbash_utils not found\e[0m" >&2
	exit 1
fi

favs=('dolphin' 'nemo' 'nautilus' 'pcmanfm' 'thunar')
cmd=$(getCommandFallbacks "${favs[@]}") || die "no file manager found in ( ${favs[*]} )"
( exec $cmd $* ) &
